<script type="text/javascript">
    
  $(document).ready(function() {
      $('#dataTableab').DataTable( {
          "lengthMenu": [ [25, 10, 50, 100, 200, -1], [25, 10, 50, 100, 200, "All"] ]
      } );
  } );
      </script>
  <!-- Page Heading -->
  <h1 class="h3 mb-2 text-gray-800">Tables</h1>
  <p class="mb-4">DataTables is a third party plugin that is used to generate the demo table below.
      For more information about DataTables, please visit the <a target="_blank" href="https://datatables.net">official DataTables documentation</a>.</p>

  <!-- DataTales Example -->
  <div class="card shadow mb-4">
      <div class="card-header py-3">
          <h6 class="m-0 font-weight-bold text-primary">Danh sách nhân viên</h6>
      </div>
      <div class="card-body">
          <div class="table-responsive">
              <div id="dataTable_wrapper" class="dataTables_wrapper dt-bootstrap4">
                      <div class="row"><div class="col-sm-12">
                  <table class="table table-bordered display dataTable" id="dataTableab" width="100%" cellspacing="0" role="grid" aria-describedby="dataTable_info" style="width: 100%;">
                  <thead>
                      <tr>
                          <th rowspan="1" colspan="1">Nhân viên</th>
                          <th rowspan="1" colspan="1">Khách hàng</th>
                          <th rowspan="1" colspan="1">Số Sản phẩm</th>
                          <th rowspan="1" colspan="1">Chứng từ</th>
                          <th rowspan="1" colspan="1">Giá</th>
                          <th rowspan="1" colspan="1">Id</th>
                          <th rowspan="1" colspan="1">kênh chăm sóc</th>
                          <th rowspan="1" colspan="1">Hành động</th>
                      </tr>
               </thead>
                  
                  <tbody>  
                          <% for(var i=0; i < excelData.length; i++) { %>
                             <tr role="row" class="odd">
                               <td><%= excelData[i].baStaff.name %></td>
                               <td><%= excelData[i].baCus.name %></td>
                               <td><%= excelData[i].product.length %></td>
                               <td><%= excelData[i].docDoc %></td>
                               <td><%= excelData[i].tranPrice %></td>
                               <td><%= excelData[i]._id %></td>
                                <td><%= excelData[i].tranPrice %></td>
                               <td>
                                  <a href="/trade/view/<%= excelData[i]._id %>" class="btn btn-success"><i class="fa fa-eye"></i></a>
                               </td>
                             </tr>
                          <% } %>
                  
                  
                  
                  
                  </tbody>
              </table>
          </div>
      </div>
      </div>
  </div>
</div>
